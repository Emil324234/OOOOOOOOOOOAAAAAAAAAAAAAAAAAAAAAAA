<Window x:Class="prct5laba5.Admin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:prct5laba5"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Admin" Height="350" Width="525">
    <Grid>
        <DataGrid x:Name="adminDataGrid" />
        <TextBox x:Name="grd" />
        <TextBox x:Name="gsa" />
        <TextBox x:Name="Update" />
        <TextBox x:Name="vcb" />
        <Button x:Name="read" Content="Read" Click="Read_Click" />
        <Button x:Name="add" Content="Add" Click="Add_Click"/>
        <Button x:Name="update" Content="Update" Click="Update_Click"/>
    </Grid>
</Window>
